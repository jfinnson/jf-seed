  float: left;
}
.hierarchy-tree li {
  list-style-type: none;
  margin: 0;
  padding: 10px 5px 0 5px;
  position: relative;
  clear: both;
  float: left;
}
.hierarchy-tree li::before,
.hierarchy-tree li::after {
  content: '';
  left: -20px;
  position: absolute;
  right: auto;
}
.hierarchy-tree .handle::before {
  border-left: 1px solid #999;
  bottom: 50px;
  height: 100%;
  top: 0;
  width: 1px;
}
.hierarchy-tree .handle::after {
  border-top: 1px solid #999;
  height: 20px;
  top: 25px;
  width: 25px;
}
.hierarchy-tree li span {
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  border: 1px solid #999;
  border-radius: 5px;
  display: inline-block;
  padding: 3px 8px;
  text-decoration: none;
}
.hierarchy-tree li span:not(.badge) {
  background: white;
}
.hierarchy-tree .tree-ctrl {
  cursor: pointer;
}
.hierarchy-tree .hierarchy-title {
  min-width: 210px;
}
