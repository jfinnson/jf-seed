}
.hierarchy-tree .new-node {
  cursor: pointer;
}
.hierarchy-tree li:last-child::before {
  height: 30px;
}
.hierarchy-tree li.parent_li > span:hover,
.hierarchy-tree li.parent_li > span:hover + ul li span {
